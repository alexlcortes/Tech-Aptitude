<div class="two-third">

<h1>Profile</h1>       

<div style="clear:both"></div>

<div>
    <div class="heading_bg"><h2>Contact Information</h2></div>
    
      <form action="/update_employee_profile?_method=PUT" class="TTWForm" method="post" novalidate=""> 
  
      <div id="field1-container" class="field f_50">
  {{#each user}}
      <input type="hidden" name="id" value="{{this.id}}">
      <label for="firstName">First Name</label>         
      <input name="firstName" id="firstName" required="required" type="text" value="{{firstName}}">
      <label for="lastName">Last Name</label>
      <input name="lastName" id="lastName" required="required" type="text" value="{{lastName}}">
      <label for="email">Email Address</label>
      <input name="email" id="email" required="required" type="text" value="{{email}}">
      <label for="address">Street Address</label>
      <input name="address" id="address" required="required" type="text" value="{{street_address}}">
      <label for="city">City</label>
      <input name="city" id="city" required="required" type="text" value="{{city}}">
      <label for="state">state</label>
      <input name="state" id="state" required="required" type="text" value="{{state}}">
      <label for="zip">Zip Code</label>
      <input name="zip" id="zip" required="required" type="text" value="{{zip}}">
    {{/each}}
      </div>
    <p><input type="submit" value="Submit"></p>
    </form>
    <label for="image">Choose an image to upload as your profile picture:</label>
    <input id="upload-input" type="file" name="uploads[]" multiple="multiple">
    <a id="uploadButton" href="#" class="button">Upload Pic</a>
    <div id="uploaded"></div>
  </div>
</div>

</div><!-- close two third -->

      <!-- Right Picture Nar -->
      <div class="sidebar_right">
        {{#each user}}
        <h4>{{firstName}} {{lastName}}</h4>
        {{/each}}
        {{#each user}}
